<ScreenNhapKho>:
    MDBoxLayout: 
        size_hint: (1,1)
        orientation: 'vertical'
        MDBoxLayout:
            id: bigbox
            orientation: 'vertical'
            size_hint: (1,0.9)
            spacing: 10
        MDBoxLayout:
            size_hint: (1,0.1)
            orientation: 'horizontal'
            spacing: 10
            Widget: 
                size_hint_x:1
            MDRaisedButton:
                text: "Xóa tất cả"
                on_press: root.delete_all()
            MDRaisedButton:
                text: "Thêm vào kho"



